@charset "utf-8";
/* CSS Document */
body{
	margin:0;
	padding:0;
	}
#barTender_wrapper{
	width:100%;
	min-height:100%;
	position:fixed;
	background-color:#000000;
	}
#barTender_section{
	width:95%;
	margin:0 auto;
	}
#barTender_header{
	width:100%;
	min-height:12.3%;
	text-align:center;
	float:left;
	}
#barTender_header h1{
	color:#c3a1db;
	}
nav{
	width:100%;
	height:24%;
	background-color:#000000;
	color:#FFF;
	}
.barTender_ul{
	display:block;
	margin:0;
	padding:0;
	}
.barTender_ul li{
	list-style:none;
	display:inline-block;
	width:12%;
	}
.barTender_ul li a{
	font-family:Myriad Pro;
	color:#c3a1db;
	
	}
.barTender_ul li a span{
	color:#fff;
	}
#barTender_content{
	width:100%;
	height:13%;
	}
#barTender_content h1{
	color:#c3a1db;
	text-align:center;
	}
#barTender_content_container{
	width:100%;
	height:46%;
	}
.barTender_container_box{
	width:17%;
	background-color:#37373a;
	margin-right:20px;
	float:left;
	}
.barTender_container_box_header{
	width:100%;
	height:8%;
	background-color:#211d23;
	float:left;
	}
.barTender_container_box_content{
	width:100%;
	height:32%;
	float:left;
	}
.barTender_container_box_content ul li{
	margin:0;
	padding:0;
	display:inline-block;
	margin-right:10px;
	}
.barTender_container_box_icons{
	width:100%;
	float:left;
	}
.barTender_container_box_icons ul li{
	}

